    "global": "^4.3.2",
    "js-base64": "^2.5.1",
    "jszip": "^3.2.1",
    "konva": "^3.4.1",
    "node-sass": "^4.13.1",
    "node-schedule": "^1.3.2",
    "query-string": "^6.8.2",
    "react": "^16.13.0",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.13.0",
